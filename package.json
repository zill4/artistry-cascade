{
  "name": "artistry-cascade",
  "version": "0.6.6",
  "description": "A powerful component library for Cascade",
  "repository": "https://github.com/zill4/artistry-cascade.git",
  "license": "MIT",
  "author": "Sean Johnson <sjohnson@sjohnsonaz.com>",
  "main": "dist/scripts/modules/ArtistryCascade.js",
  "typings": "dist/scripts/modules/ArtistryCascade.d.ts",
  "scripts": {
    "start": "node ./dist/main.js",
    "build": "tsc --declaration",
    "clean": "rimraf ./dist",
    "prepublishOnly": "rimraf ./dist && tsc --declaration",
    "test": "tsc && node src/mocha/NodeRunner.js",
    "min": "webpack --config webpack.config.js --progress",
    "dev": "webpack --config webpack.dev.config.js --progress",
    "watch": "webpack --config webpack.dev.config.js --watch --progress",
    "watch-link": "tsc --watch --declaration",
    "node": "node ./dist/scripts/main.js",
    "open": "opn src/tests/demo/html/index.html"
  },
  "dependencies": {
    "@artistry/icons": "https://github.com/zill4/artistry-icons#main",
    "@cascade/diff": "https://github.com/zill4/cascade-diff#main",
    "@cascade/promise-util": "https://github.com/zill4/cascade-promise-util#main"
  },
  "peerDependencies": {
    "artistry": "https://github.com/zill4/artistry#main",
    "cascade": "https://github.com/zill4/cascade#main"
  },
  "devDependencies": {
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@types/mocha": "^10.0.1",
    "artistry": "https://github.com/zill4/artistry#main",
    "autoprefixer": "^10.4.16",
    "cascade": "https://github.com/zill4/cascade#main",
    "css-loader": "^6.8.1",
    "jsdom": "^22.1.0",
    "mini-css-extract-plugin": "^2.7.6",
    "mocha": "^10.2.0",
    "opn-cli": "5.0.0",
    "postcss-loader": "^7.3.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "^5.0.1",
    "roboto-fontface": "0.10.0",
    "style-loader": "^3.3.3",
    "stylus": "^0.60.0",
    "stylus-loader": "^7.1.3",
    "ts-loader": "^9.4.4",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "component",
    "button",
    "modal",
    "input",
    "cascade",
    "reactive",
    "view",
    "viewmodel",
    "mvvm",
    "mvc",
    "template",
    "dom",
    "react",
    "knockout"
  ]
